{
  "manifest_version": 3,
  "name": "[PRE]fixed â€“ Tab Title Prefixer",
  "version": "5.1.2",
  "description": "Readable, prioritised prefixes for browser tabs.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "manager.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "manager.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}